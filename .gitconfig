[core]
	autocrlf = false
	eol = lf
    editor = code --wait
[merge]
    tool = vscode
[mergetool "vscode"]
    cmd = code --wait $MERGED
[diff]
    tool = vscode
[difftool "vscode"]
    cmd = code --wait --diff $LOCAL $REMOTE
[difftool]
    prompt = false
[credential]
    helper = cache --timeout 60000
